{
    "name": "hungnm144/laravel-elfinder",
    "keywords": ["laravel", "elfinder", "tinymce", "filebrowser"],
    "description": "A Laravel Package to integrate elFinder 2",
    "license": "MIT",
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "^5.0",
        "studio-42/elfinder": "^2.1",
        "barryvdh/elfinder-flysystem-driver": "^0.1.4|^0.2",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hungnm\\Elfinder\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        },
        "laravel": {
            "providers": [
                "Hungnm\\Elfinder\\ElfinderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
